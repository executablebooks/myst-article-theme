{
  "name": "@myst-theme/article",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "build": "npm run build:css && remix build",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "start": "npm run build:css && remix dev"
  },
  "dependencies": {
    "@myst-theme/icons": "^0.1.1",
    "@myst-theme/jupyter": "^0.1.23",
    "@myst-theme/providers": "^0.1.23",
    "@myst-theme/site": "^0.1.23",
    "@myst-theme/styles": "^0.0.1",
    "@remix-run/node": "^1.12.0",
    "@remix-run/react": "^1.12.0",
    "@remix-run/vercel": "^1.12.0",
    "css-selector-parser": "^1.4.1",
    "myst-common": "^0.0.12",
    "myst-config": "^0.0.9",
    "node-fetch": "^2.6.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.12.0",
    "@remix-run/eslint-config": "^1.12.0",
    "@remix-run/serve": "^1.12.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vercel/node": "^2.9.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.23.1",
    "prettier": "latest",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=14"
  },
  "packageManager": "npm@8.10.0",
  "overrides": {
    "ansi-to-react": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    }
  }
}
